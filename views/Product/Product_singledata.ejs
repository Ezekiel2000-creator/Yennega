<%-include('Admin_partial/header')-%>
            <!-- HEADER DESKTOP-->   
            <!-- MAIN CONTENT-->
<div class="main-content">
                <div class="section__content section__content--p30">
                        <a href="/customer/data_display" ><img src="/images/icons/back.png" width="20px" style="margin-left: 20px;margin-top: 20px; margin-bottom: 20px" title="back"alt="back"></a>
                    <div class="container-fluid">
                            <div class="row">
                                <div class="table-responsive m-b-40">
                                    <table class="table table-bordered table-data">
                                            <tr>
                                                <td>id du produit</td>
                                                <td><%=product_array.Product_id%></td>
                                            </tr>
                                            <tr>    
                                                <td>Nom du produit</td>
                                                <td><%=product_array.Pro_name%></td>
                                            </tr>
                                            <tr>    
                                                <td>Vendeur du produit</td>
                                                <td>
                                                    <a href="/customer/show/<%=product_array.Pro_vendor._id %>">
                                                        <%=product_array.Pro_vendor.Customer_first_name%> <%=product_array.Pro_vendor.Customer_last_name%> 
                                                    </a>
                                                    
                                                </td>
                                            </tr>    
                                            <tr>
                                                <td>Quantité disponible</td>
                                                <td><%=product_array.Pro_quantity%></td>
                                            </tr>
                                            <tr>
                                                <td>Image du produit</td>
                                                <td><img width="100px" src="/Pro_upload/<%=product_array.Pro_image %>"> </td>
                                            </tr>
                                            <tr>
                                                <td>Date de création</td>
                                                <td><%=product_array.Pro_date%></td>
                                            </tr>
                                            <tr>    
                                                <td>Prix de vente</td>
                                                <td><%=product_array.Pro_rentprice%> FCFA</td>
                                            <tr>    
                                                <td>Prix du produit</td>
                                                <td><%=product_array.Pro_ammount%> FCFA</td>
                                            </tr>
                                    </table>
                                </div>
                        </div>
                    </div>  
                </div>     
            </div>
        </div>
    </div>
     <!-- plugins:js -->
  <script src="/vendors/js/vendor.bundle.base.js"></script>
  <script src="/vendors/js/vendor.bundle.addons.js"></script>
  <!-- endinject -->
  <!-- inject:js -->
  <script src="/js/off-canvas.js"></script>
  <script src="/js/hoverable-collapse.js"></script>
  <script src="/js/misc.js"></script>
<script src="/js/admin.js"></script>
  <script src="/js/settings.js"></script>
  <script src="/js/todolist.js"></script>
  <!-- endinject -->
</body>
