<%-include('Admin_partial/header')-%>
            <!-- HEADER DESKTOP-->
            <!-- MAIN CONTENT-->
        <div class="main-panel">
            <div class="content-wrapper">
                    <div class="page-header" style="margin-bottom:30px; margin-right:30px">
                            <h3 class="page-title" style="margin-left:200px;padding-top:20px">
                            <b>Formulaire de modification des sous catégorie</b>
                            </h3>
                            <a href="/customer/data_display" ><img src="/images/icons/back.png" width="30px" style="margin-left: 20px;margin-top: 20px; margin-bottom: 20px" title="back"alt="back"></a>
                    </div>
            <div class="row">
                <div class="col-lg-12">
                    <div class="card">
                    <div class="card-body">
                        <h4 class="card-title">Modifier la sous-catégorie</h4>
                        <form method="POST">
                                <label for="SubCategory_id" class=" form-control-label">Id de la sous catégorie</label>
                                <input type="text" name="subcate_id" value="<%=subcategory_array.subcate_id %>"class="form-control">
                            
                                <label for="SubCategory_name" class=" form-control-label">Nom de la sous catégorie</label>
                                <input type="text" name="subcate_name" value="<%=subcategory_array.subcate_name %>" class="form-control">
                                <div class="card-body card-block">
                                    <lable>Quels catégories de produit avez-vous?</lable>
                                                <select  name="category">
                                                <% for(var i=0;i<category_array.length;i++) { %>
                                                <option  value="<%= category_array[i]._id%>"><%=category_array[i].cate_name%> </option>
                                                <%  } %>
                                                </select>
                                <button type="submit" class="btn btn-primary btn-sm">
                                    <i class="fa fa-dot-circle-o"></i> Enregistrement
                                </button>
                                <button type="reset" class="btn btn-danger btn-sm">
                                    <i class="fa fa-ban"></i> Abandonner
                                    </button>
                                    </div>
                            </form>
                    </div>
                    </div>
                </div>
            </div>
        </div>
            <!-- content-wrapper ends -->
            <!-- partial:../../partials/_footer.html -->
            <footer class="footer">
        <div class="d-sm-flex justify-content-center justify-content-sm-between">
                <span class="text-muted text-center text-sm-left d-block d-sm-inline-block">Copyright © 2018 <a href="https://www.urbanui.com/" target="_blank">Urbanui</a>. All rights reserved.</span>
                <span class="float-none float-sm-right d-block mt-1 mt-sm-0 text-center">Hand-crafted & made with <i class="mdi mdi-heart text-danger"></i></span>
        </div>
            </footer>
            <!-- partial -->
        </div>
        <!-- main-panel ends -->
        </div>
        <!-- page-body-wrapper ends -->
    </div>
    <!-- container-scroller -->
    <!-- plugins:js -->
    <script src="/vendors/js/vendor.bundle.base.js"></script>
    <script src="/vendors/js/vendor.bundle.addons.js"></script>
    <!-- endinject -->
